<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
     
    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.7.min.js" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.8.16/jquery-ui.min.js" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.9/jquery.validate.min.js" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/modernizr/modernizr-1.7-development-only.js" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    
    <script src="@Url.Content("~/Scripts/jquery/plugins/form/jquery.form.js")" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/modernizr/modernizr-1.7-development-only.js" type="text/javascript"></script>
   
    <link href="@Url.Content("~/Content/css/Site.css")" rel="stylesheet" type="text/css" />
    <link href="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.8.16/themes/smoothness/jquery-ui.css" rel="stylesheet" type="text/css" />

    @Html.Partial("~/Views/Shared/Css/css.cshtml")
    
   
</head>
<body>
    <div id="Container">
        <div id="NavBar">
            <div id="AppName">
                <a href="@Url.Action("Index","Home")">
                    <img src="@Url.Content("~/Content/images/header/chart.png")"/>
                </a>
               
            </div>
            <ul>
                <li>@Html.ActionLink("About Reputrade","Index","Home")</li>
                <li>@Html.ActionLink("News","Index","Home")</li>
                <li>@Html.ActionLink("Contact Us","Index","Home")</li>
            </ul>
        </div>
		<div id="Body">
			@RenderBody()
		</div>
	</div>
</body>
</html>
